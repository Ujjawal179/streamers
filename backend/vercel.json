{
  "version": 2,
  "builds": [
    {
      "src": "src/api/**/*.ts",
      "use": "@vercel/node"
    },
    {
      "src": "src/controllers/**/*.ts",
      "use": "@vercel/node"
    },
    {
      "src": "src/db/**/*.ts",
      "use": "@vercel/node"
    },
    {
      "src": "src/helpers/**/*.ts",
      "use": "@vercel/node"
    },
    {
      "src": "src/middleware/**/*.ts",
      "use": "@vercel/node"
    },
    {
      "src": "src/router/**/*.ts",
      "use": "@vercel/node"
    },
    {
      "src": "src/zod/**/*.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "src/api/$1.ts"
    }
  ]
}
